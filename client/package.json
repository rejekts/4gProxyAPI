{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.0",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0"
  },
  "scripts": {
    "start": "export PORT=5000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serverA": "nodemon ../serverA/index.js",
    "serverB": "nodemon ../serverB/index.js --ignore client",
    "allServers": "concurrently \"npm run serverA\" \"npm run serverB\"",
    "all": "concurrently \"npm run serverA\" \"npm run serverB\" \"npm run start\""
  },
  "proxy": "http://127.0.0.1:10080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
